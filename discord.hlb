export sdk
export csharp

fs csharp() {
    copy sdk "/csharp" "/"
}

fs unzipSDK() {
	image "alpine"
	run "unzip /in/discord_game_sdk.zip -d /out" with option {
		mount sdkZip "/in"
		mount scratch "/out" as sdk
	}
}

fs sdkZip() {
	http "https://dl-game-sdk.discordapp.net/2.5.6/discord_game_sdk.zip"
}